<script src="http://code.highcharts.com/highcharts.js"></script>
<script src="http://code.highcharts.com/modules/exporting.js"></script>
<script src="/js/graph.js"></script>

  <%= @survey.name %> Survey

  <%= @survey.survey_takers.count %> Responses

<div class="question-review">
  <table>
  <% @survey.questions.each do |q| %>
    <tr> 
      <td>
      <%= q.question %>
      <td>
    </tr>  
    <tr>
      <% q.choices.each do |c| %> 
      <td>
        <%= c.choice %> 
      </td>
      <td>  
        <%= c.responses.where(c.id == response.choice_id).count %> 
      <td>  
      <% end %>
      
      <div id="question-response" style="min-width: 410px; height: 500px; margin: 0 auto"></div>
      <% end %>
</table>
</div>


  
 
